  <!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    
    
<% for (let i = 1; i <= Math.ceil(total / perPage); i++) { %>
    <% if(i === page) { %>
      <span><%= i %></span>
    <% } else { %>
      <a href="/booking/paginate?page=<%= i %>&perPage=<%= perPage %>"><%= i %></a>
    <% } %>
  <% } %>

  </body>
</html>
